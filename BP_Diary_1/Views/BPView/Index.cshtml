@using BP_Diary_1.Models;
@model IEnumerable<bp_diary_records>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h4>BP Diary Records</h4>
    <hr />
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>Customer Id</th>
            <th>ContactName</th>
            <th>City</th>
            <th>Country</th>
        </tr>
        @foreach (bp_diary_records bp_diary in Model)
        {
            <tr>
                <td>@bp_diary.bp_diary_id</td>
                <td>@bp_diary.date_record</td>
                <td>@bp_diary.systolic</td>
                <td>@bp_diary.diastolic</td>
            </tr>
        }
    </table>
</body>
</html>